fi

if [ $COIN == "SMART" ]
then
HCD='/home/m1/Miners/ccminer/ccminer'
ADDR="$SMART_ADDRESS.$SMART_WORKER"
screen -dmSL miner $HCD -a keccak -o $SMART_POOL -u $ADDR -p $MINER_PWD -i $OCIT
