#!/bin/bash
########## DONT TOUCH BELOW THIS LINE ############
source ~/1bash
source ~/Resources
3Main_Coin_Sequence(){
CHECK_CARD
CHECK_I
miner_fix2
if [ $WTM_AUTO_SWITCH == "YES" ]; then
source ~/Logs/WTM_ccoin
COIN=$CCOIN
fi
########## DONT TOUCH ABOVE THIS LINE ############
########## ADD COINS BELOW THIS LINE #############
if [ $COIN == "NICEHASH_NEOSCRYPT" ]
then
 HCD='/home/m1/Miners/KTccminer/ccminer'
 ADDR="$NICEHASH_BTC_ADDRESS.$NICEHASH_WORKER"
 screen -dmSL miner $HCD -a neoscrypt -o stratum+tcp://$NICEHASH_NEOSCRYPT_POOL:$NICEHASH_NEOSCRYPT_PORT -u $ADDR -p x -i $NICEHASH_NEOSCRYPT_INTENSITY
fi

if [ $COIN == "NICEHASH_LYRA2REV2" ]
then
 HCD='/home/m1/Miners/alccminer/ccminer' miner_start1 
 ADDR="$NICEHASH_BTC_ADDRESS.$NICEHASH_WORKER"
 screen -dmSL miner $HCD -a lyra2v2 -o stratum+tcp://$NICEHASH_LYRA2REV2_POOL:$NICEHASH_LYRA2REV2_PORT -u $ADDR -p x -i $NICEHASH_LYRA2REV2_INTENSITY 
fi

if [ $COIN == "NICEHASH_X11GOST" ]
then
 HCD='/home/m1/Miners/SPccminer/ccminer' 
 ADDR="$$NICEHASH_BTC_ADDRESS.$NICEHASH_WORKER" 
 screen -dmSL miner $HCD -a sib -o stratum+tcp://$NICEHASH_X11GOST_POOL:$NICEHASH_X11GOST_PORT -u $ADDR -p x -i $NICEHASH_X11GOST_INTENSITY 
fi

