then
 HCD='/home/m1/Miners/TPccminer/ccminer'
 ADDR="$NICEHASH_BTC_ADDRESS.$NICEHASH_WORKER"
 screen -dmSL miner $HCD -a skunk -o stratum+tcp://$NICEHASH_SKUNKHASH_POOL:$NICEHASH_SKUNKHASH_PORT -u $ADDR -p x -i $NICEHASH_SKUNKHASH_INTENSITY 
fi

if [ $COIN == "NICEHASH_CRYPTONIGHT" ] 
then
 HCD='/home/m1/Miners/KTccminer-cryptonight/ccminer'
 ADDR="$NICEHASH_BTC_ADDRESS.$NICEHASH_WORKER"
 screen -dmSL miner $HCD -o stratum+tcp://$NICEHASH_CRYPTONIGHT_POOL:$NICEHASH_CRYPTONIGHT_PORT -u $ADDR -p x -i $NICEHASH_CRYPTONIGHT_INTENSITY 
