if [ $ZM_or_EWBF == "ZM" ]
then
HCD='/home/m1/Miners/zec/zm/5_8/zm'
screen -dmSL miner $HCD --server $MNX_POOL --user $MNXADDR --pass $MINER_PWD --port $MNX_PORT  --time $ZM_OPTS ;
fi

