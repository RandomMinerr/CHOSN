then
HCD='/home/m1/Miners/eth/ethminer_12dev2/ethminer'

NICEADDR="$BTC_ADDRESS.$NICE_ETHASH_WORKER"

screen -dmSL miner $HCD -SP 2 -S $GENOIL_NICE_ETHASH_POOL -O $NICEADDR:x -U
